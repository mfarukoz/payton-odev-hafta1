a=1
while a<12:
    if a==9:
        print('*'*(12-a),' '*2,'pycoders.nl',' '*3,'*'*(12-a),sep='')
    elif a==11:
        print('*',' '*2,'='*11,' '*3,'*')
    else:
        print('*'*(12-a),' '*(2*(a-1)),'*'*(12-a),sep='')      
    a+=2

b=1
while b<11:
    if b==1:
        print('*'*(b+2),' '*6,'mfq',' '*7,'*'*(b+2),sep='')
    else:
        print('*'*(b+2),' '*(2*(9-b)),'*'*(b+2),sep='')
    b+=2
